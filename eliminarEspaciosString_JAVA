public static void main(String[] args) {

        String contraseñaEspacios = "skdjKsjl   KDSJLKSDJALakka kajsdkA 441 443 33";
        String contraseñaSinEspacios = "";

        String[] split = contraseñaEspacios.split(" ");

        for (int i = 0; i < split.length; i++) {
            contraseñaSinEspacios += split[i];
        }

        System.out.println(contraseñaSinEspacios); //skdjKsjlKDSJLKSDJALakkakajsdkA44144333
    }
